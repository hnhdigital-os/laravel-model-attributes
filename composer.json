{
    "name": "hnhdigital-os/laravel-model-schema",
    "description": "Changes how the Eloquent Model provides attributes.",
    "keywords": ["laravel","illuminate","attributes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/hnhdigital-os/laravel-null-carbon.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": ">=5.5",
        "genealabs/laravel-null-carbon": "dev-master"
    },
    "require-dev": {
        "illuminate/database": "4.*|5.*",
        "phpunit/phpunit": "^7.0",
        "symfony/thanks" : "*"
    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\ModelSchema\\": "src/",
            "HnhDigital\\ModelSchema\\Tests\\": "src/tests/"
        }
    },
    "scripts": {
        "test": [
            "composer phpunit && composer phpcs && composer phpmd"
        ],
        "phpunit": [
            "phpunit"
        ],
        "phpcs": [
            "phpcs -p -s --standard=PSR2 ./src ./tests"
        ],
        "phpmd": [
            "phpmd ./src phpmd.xml"
        ]
    }
}
